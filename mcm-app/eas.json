{
  "cli": {
    "version": ">= 16.7.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "GOOGLE_SERVICES_JSON": "GOOGLE_SERVICES_JSON"
      },
      "hooks": {
        "prebuild": "node scripts/write-google-services-json.js"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    },
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "GOOGLE_SERVICES_JSON": "GOOGLE_SERVICES_JSON"
      },
      "hooks": {
        "prebuild": "node scripts/write-google-services-json.js"
      },
      "environment": "development"
    }
  },
  "submit": {
    "production": {}
  }
}
